public class rrr {
    public static void main(String[] args) {
        int []list={1,4,8,5,9,7};
        int i=linearserch(list,8);
        System.out.println("The key found at index"+i);
    }
    public static int linearserch(int []list,int k){
        for(int i=0;i<list.length;i++){
            if(list[i]==k){
                return i;
            }
        }
        return -1;
    }
}
